#!/bin/bash -eu

#SBATCH --job-name papi-try
#SBATCH --time=0:30:00
#SBATCH --nodes=1
#SBATCH --constraint=knl,quad,flat
#SBATCH --partition=knl
#SBATCH --output=/users/%u/joblogs/papi-try-%j.out

PPN=64
NUMA_OPTS="-m 1"
EVENTS="PAPI_L2_DCA PAPI_L2_DCM"

@CMAKE_INSTALL_PREFIX@/scripts/run_papi.sh "ppn=$PPN" \
    "numa_opts=$NUMA_OPTS" "events=$EVENTS"

exit 0
